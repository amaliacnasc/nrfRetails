        "backgroundColor": "#ffffff"
      },
      "package": "com.miguelamaral254.nrfRetails",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7f56f8da-5504-49b3-bb9b-57f198875a8f"
